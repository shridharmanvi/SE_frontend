<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sagar.findout"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="21" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="FindOut"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.sagar.findout.MainActivity"
            android:label="FindOut"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.example.sagar.findout.ResultPercentage"
            android:label="@string/title_activity_result_percentage" >
        </activity>
        <activity
            android:name="com.example.sagar.findout.Splash"
            android:label="FindOut" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>